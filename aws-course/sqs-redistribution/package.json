{
	"name": "sqs-redistribution",
	"version": "1.0.0",
	"description": "sqs-redistribution",
	"main": "serverless.yml",
	"scripts": {
		"sls": "sls",
		"dev:deploy": "serverless deploy --verbose --stage dev",
		"dev:undeploy": "serverless remove --stage dev",
		"dev:function": "npm run sls deploy -- function -f --stage dev",
		"sit": "npm run sls deploy -- --verbose --stage sit",
		"sit:undeploy": "npm run sls remove --stage sit",
		"sit:function": "npm run sls deploy -- function -f --stage sit",
		"lint": "eslint --ext .ts ./src --fix"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"dependencies": {
		"@hapi/boom": "^9.1.4",
		"content-type": "^1.0.4",
		"joi": "^17.6.0",
		"luxon": "^2.4.0",
		"middy": "^0.36.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^3.2.0",
		"@types/aws-lambda": "^8.10.71",
		"@types/content-type": "^1.1.5",
		"@types/luxon": "^2.3.2",
		"@types/node": "^14.14.25",
		"@types/uuid": "^8.3.4",
		"@types/webpack-node-externals": "^2.5.3",
		"aws-sdk": "^2.1144.0",
		"eslint": "^7.8.1",
		"eslint-config-airbnb-typescript": "^8.0.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.5.1",
		"serverless": "^3.18.2",
		"serverless-layers": "^1.5.0",
		"serverless-webpack": "^5.6.0",
		"ts-loader": "^8.0.15",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"tsconfig-paths-webpack-plugin": "^3.3.0",
		"typescript": "^4.1.3",
		"webpack": "^5.65.0",
		"webpack-node-externals": "^3.0.0"
	},
	"author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
